<p id="notice"><%= notice %></p>

<h5>Origens</h5>
<%= link_to 'Nova Origem',new_source_path, class: 'waves-effect waves-light btn-small blue'%>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sources.each do |source| %>
      <tr>
        <td><%= source.name %></td>
        <td><%= link_to 'Editar', edit_source_path(source) %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

